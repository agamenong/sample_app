<h1>Sign in</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>

	<div classs="field"> 
	    <%= f.label :email %><br />
	    <%= f.text_field :email %>
	</div>

	<div classs="field"> 
	    <%= f.label :password %><br />
	    <%= f.password_field :password %>
	</div>
	
	<div classs="actions"> 
	        <%= f.submit "Sign in" %>
    </div>
	
<% end %>	

<p>New User? <%= link_to "Sign up now!", signup_path %> </p>
	
